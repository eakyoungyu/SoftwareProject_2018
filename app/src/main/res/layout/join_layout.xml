<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/join_editText_email"
        android:layout_marginStart="33dp"
        android:gravity="center"
        android:text="이메일"
        android:textColor="#00695c"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="55dp" />

    <EditText
        android:id="@+id/join_editText_email"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="90dp"
        android:background="@drawable/rounded"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="55dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/textView9"
        android:layout_alignTop="@+id/join_editText_password"
        android:gravity="center"
        android:text="PW"
        android:textColor="#00695c"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="125dp" />


    <TextView
        android:id="@+id/textView8"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/textView9"
        android:layout_alignTop="@+id/join_editText_password_check"
        android:gravity="center"
        android:text="PW확인"
        android:textColor="#00695c"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="175dp" />


    <EditText
        android:id="@+id/join_editText_password"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="160dp"
        android:background="@drawable/rounded"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="125dp" />

    <EditText
        android:id="@+id/join_editText_password_check"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="230dp"
        android:background="@drawable/rounded"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="175dp" />

    <Button
        android:id="@+id/join_button_pw_check"
        android:layout_width="65dp"
        android:layout_height="36dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView8"
        android:layout_marginEnd="13dp"
        android:text="확인"
        android:background="@drawable/rounded"
        android:textColor="#00796B"
        tools:layout_editor_absoluteX="334dp"
        tools:layout_editor_absoluteY="43dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="50dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/textView9"
        android:layout_alignTop="@+id/join_editText_name"
        android:gravity="center"
        android:text="이름"
        android:textColor="#00695c"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="245dp" />

    <EditText
        android:id="@+id/join_editText_name"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="300dp"
        android:background="@drawable/rounded"
        android:ems="10"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="245dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="60dp"
        android:layout_height="40dp"
        android:layout_alignEnd="@+id/textView9"
        android:layout_alignTop="@+id/join_editText_goal"
        android:gravity="center"
        android:text="목표체중"
        android:textColor="#00695c"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="295dp" />

    <EditText
        android:id="@+id/join_editText_goal"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="195dp"
        android:background="@drawable/rounded"
        android:ems="10"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="295dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="60dp"
        android:layout_height="30dp"
        android:layout_alignBottom="@+id/textView12"
        android:layout_alignEnd="@+id/textView9"
        android:gravity="center"
        android:text="트레이너"
        android:layout_marginBottom="-60dp"
        android:textColor="#00695c"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="345dp" />

    <Button
        android:id="@+id/join_button_trainer"
        android:layout_width="65dp"
        android:layout_height="35dp"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/textView13"
        android:layout_marginStart="104dp"
        android:background="@drawable/rounded"
        android:text="선택"
        android:textColor="#00796B"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="335dp" />

    <Button
        android:id="@+id/join_button_join"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="46dp"
        android:text="가입하기"
        android:textColor="#ffffff"
        android:background="@drawable/rounded2"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="415dp" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="300dp"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="11dp"
        android:text="Welcome!"
        android:textAlignment="center"
        android:textColor="#00695c"
        android:textSize="30dp"/>

    <TextView
        android:id="@+id/join_selected_trainer"
        android:layout_width="69dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView13"
        android:layout_marginEnd="89dp"
        android:text="-"
        android:visibility="invisible" />

</RelativeLayout>