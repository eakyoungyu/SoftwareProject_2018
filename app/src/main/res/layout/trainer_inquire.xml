                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/trainerInquireUI_imageView_image1"
                        android:layout_width="107dp"
                        android:layout_height="100dp"
                        android:text="사진" />

                    <TextView
                        android:id="@+id/trainerInquireUI_textView_name1"
                        android:layout_width="107dp"
                        android:layout_height="27dp"
                        android:text="이름" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/trainerInquireUI_textView_labelType1"
                            android:layout_width="32dp"
                            android:layout_height="23dp"
                            android:text="강도" />

                        <TextView
                            android:id="@+id/trainerInquireUI_textView_type1"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:text="상/중/하" />
                    </LinearLayout>

                    <RadioButton
                        android:id="@+id/trainerInquireUI_radioButton_selectTrainer1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text=""
                        android:textStyle="bold" />

                </LinearLayout>

